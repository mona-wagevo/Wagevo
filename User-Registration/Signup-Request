func insertUserIntoTable(userId: UUID, email: String, phone: String, firstName: String, lastName: String, roleId: String, birthday: String, hourlyWage: String) async {
    do {
        let response = try await supabase.database
            .from("Users")
            .insert([
                "id": userId.uuidString,
                "email": email,
                "phone": phone,
                "first_name": firstName,
                "last_name": lastName,
                "role_id": roleId,
                "birthday": birthday,
                "hourly_wage": hourlyWage
            ])
            .execute()
        
        print("User inserted into Users table: \(response)")
    } catch {
        print("Error inserting user: \(error)")
    }
}
