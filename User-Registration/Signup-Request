import Supabase

let supabase = SupabaseClient(
    supabaseURL: URL(string: "https://your-project-ref.supabase.co")!,
    supabaseKey: "your-anon-key"
)

func signUpUser(email: String, password: String, phone: String, firstName: String, lastName: String, roleId: String, birthday: String, hourlyWage: String) async {
    do {
        let response = try await supabase.auth.signUp(
            email: email,
            password: password,
            data: [
                "phone": phone,
                "first_name": firstName,
                "last_name": lastName,
                "role_id": roleId,
                "birthday": birthday,
                "hourly_wage": hourlyWage
            ]
        )
        print("User signed up successfully: \(response)")
    } catch {
        print("Error signing up: \(error)")
    }
}
